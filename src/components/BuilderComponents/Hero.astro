---
import Button from "../Button.astro"
import PrimaryHero from "./Heroes/PrimaryHero.astro"
import SecondaryHero from "./Heroes/SecondaryHero.astro"
const { title, description, buttons, image, style } = Astro.props
console.log({style})
---

{
  () => {
    switch (style) {
      case 'primary':
        return <PrimaryHero {...Astro.props} />    
      case 'secondary':
        return <SecondaryHero {...Astro.props} />
      default:
        break;
    }
 
  }
}

